FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/fluidRegion";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 900;

boundaryField
{
    topWall
    {
        //type            zeroGradient;
        type    fixedValue;
        value   uniform 900;
    }
    bottomWall
    {
        //type            zeroGradient;
        type    fixedValue;
        value   uniform 900;
    }
    leftWall
    {
        //type            zeroGradient;
        type    fixedValue;
        value   uniform 900;
    }
    rightWall
    {
        //type            zeroGradient;
        type    fixedValue;
        value   uniform 900;
    }
    frontAndBack
    {
        type            empty;
    }
}


