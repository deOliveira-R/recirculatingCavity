FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/fluidRegion";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 1 -1 0 0 0 0 ];

internalField   uniform ( 0 0 0 );

boundaryField
{
    topWall
    {
        type            fixedValue;
        value           uniform ( -0.5 0 0 );
    }
    bottomWall
    {
        type            fixedValue;
        value           uniform (  0.5 0 0 );
    }
    leftWall
    {
        type            fixedValue;
        value           uniform ( 0 -0.5 0 );
    }
    rightWall
    {
        type            fixedValue;
        value           uniform ( 0 0.5 0 );
    }
    frontAndBack
    {
        type            empty;
    }
}
